using namespace std;
struct polya
{
    string usword;
    string conword;
    int weight;

};
string convert (string vvo)
{
    int k=0;
for (int i=0;i<vvo.size();i++)
    k++;
int up=k-1,low=0,k1=0,i;
    int last,tmb;




     for (  i = 0; i < k; i++)
        if (vvo[0] == vvo[i])
            k1++;


    while (low<up){
            last=-1;//несущ индекс
        for ( i=low;i<up;i++)
        {
            if (vvo[i]>vvo[i+1])
            {
               tmb=vvo[i];
               vvo[i]=vvo[i+1];
               vvo[i+1]=tmb;

           last=i; }
        }
        up=last;


        last= k;
        for(i=up-1;i>=low;i--)
        {
            if (vvo[i]>vvo[i+1])
            {
                tmb=vvo[i];
               vvo[i]=vvo[i+1];
               vvo[i+1]=tmb;
               last=i;}

        }

        low=last+1;
        }


return vvo;
}

