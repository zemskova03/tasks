int main(){
setlocale(LC_ALL,"RUS");

polya mas[140];
string vvod,vvo="p";

cout<<"Введите слово (работаем только на английском языке)"<<endl;

 getline (cin,vvo);

 int c=0,m=0;

 for(int i=0; i<sizeof(vvo)-1; i++)
if(vvo[i]>='A' && vvo[i]<='Z') vvo[i]+='z'-'Z';

if (vvo.size()==1)
{cout<<"Вы ввели всего одну букву"<<endl;return 1;}




for(int i=0; i<vvo.size(); i++)
    if (vvo[0]==vvo[i])c++;

if (c==vvo.size())
    {cout<<"все буквы одинаковые"<<endl;return 1;}
c=0;
 vvod=convert (vvo);
perenos(mas);

for (int i=0;i<140;i++)
    if (vvod==mas[i].conword) c++;

if (c==0)
{cout<<"такого слова не существует в словаре"<<endl;return 1;}
int u=0;

for (int i=0;i<140;i++)
 if (vvod==mas[i].conword)
    u=mas[i].weight;

for (int i=0;i<140;i++)
 if ((vvod==mas[i].conword)&&(u<mas[i].weight))
    u=mas[i].weight;

for (int i=0;i<140;i++)
 if ((vvod==mas[i].conword)&&(u==mas[i].weight))
    vvod=mas[i].usword;


cout<<"Получилось:"<<endl;
 cout<<vvod;



return 0;
}
